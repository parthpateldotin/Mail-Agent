{
  "name": "aimail",
  "version": "1.0.0",
  "description": "AI-powered email management system",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "ts-node src/scripts/migrate.ts",
    "migration:revert": "npm run typeorm migration:revert",
    "db:seed": "ts-node src/scripts/seed.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "@slack/web-api": "^7.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.2",
    "@types/ws": "^8.5.14",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "imap": "^0.8.19",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.25",
    "mailparser": "^3.6.9",
    "ml-kmeans": "^6.0.0",
    "ml-matrix": "^6.12.0",
    "ml-random-forest": "^2.1.0",
    "ml-regression": "^6.2.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.12",
    "openai": "^4.29.0",
    "pg": "^8.11.3",
    "recharts": "^2.15.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.11.25",
    "@types/nodemailer": "^6.4.14",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
